# Default Configuration for EnvKey Lite
# These values are used as fallbacks when not specified in environment

# Application Configuration
APP_NAME=EnvKey Lite
APP_VERSION=1.0.0
NODE_ENV=development
PORT=3000
HOST=localhost

# Database Configuration
# DATABASE_DIR is intentionally not set here to use in-memory by default

# Security Configuration
API_KEY_PREFIX=envkey_

# CORS Configuration
CORS_ORIGIN=*

# Logging Configuration
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Session Configuration
SESSION_TIMEOUT_HOURS=24

# Audit Log Configuration
AUDIT_LOG_RETENTION_DAYS=90

# Admin User Configuration
ADMIN_NAME=Administrator

# Health Check Configuration
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_PATH=/health